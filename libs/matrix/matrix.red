#! /usr/local/bin/redRed [	Title:		"Matrices with objects"	Author:		"F. Jouen"	Rights:		"Copyright (c) 2015 F. Jouen. All rights reserved."	License:    "BSD-3 - https://github.com/dockimbel/Red/blob/master/BSD-3-License.txt"]mx!: make object! [	msize: 	[pair!]	munit:	[integer!]	mvec:	[vector!]	init: does [		mvec: make vector! reduce ['integer! munit msize/x * msize/y]	]	rand: func [value] [		n: length? mvec 		i: 1		while [i <= n] [ mvec/(i): random value i: i + 1]	]]mat: make object! mx!mat/msize: 128x128mat/munit: 32mat/initmat/rand FFhprobe mat/mvec 