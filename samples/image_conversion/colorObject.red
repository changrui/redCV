Red [	Title:   "Colors "	Author:  "Francois Jouen"	File: 	 %colorObject.red	Needs:	 'View]; last Red Master required!#include %../../libs/redcv.red ; for redCV functionsmargins: 10x10img1: rcvLoadImage %../../images/billes.jpgsrc:  rcvCloneImage img1dst:  rcvCloneImage img1lower:  0.0.0upper:  255.255.255to-text: function [val][form to integer! 0.5 + 255 * any [val 0]]to-color: function [r g b][	color: 0.0.0	if r [color/1: to integer! 256 * r]	if g [color/2: to integer! 256 * g]	if b [color/3: to integer! 256 * b]	color]; ***************** Test Program ****************************view [	title "Object Color Detection"	style txt: text 40 right	style value: text "0" 30 right bold	; lower tupple	txt "Red:" lrSl: slider 256 value react [face/text: to-text lrSl/data] return	txt "Green:" lgSl: slider 256 value react [face/text: to-text lgSl/data] return	txt "Blue:" lbSl: slider 256 value react [face/text: to-text lbSl/data] 	pad 0x-65 box1: base 150x90 react [face/color: to-color lrSl/data lgSl/data lbSl/data] return		;upper tupple	txt "Red:" urSl: slider 256 value react [face/text: to-text urSl/data] return	txt "Green:" ugSl: slider 256 value react [face/text: to-text ugSl/data] return	txt "Blue:" ubSl: slider 256 value react [face/text: to-text ubSl/data] 	pad 0x-65 box2: base 150x90 react [face/color: to-color urSl/data ugSl/data ubSl/data] return		txt "Use BGR -> HSV transform? " cb: check  [		either face/data [rcvBGR2HSV img1 src][rcvCopyImage img1 src]	]			return	canvas:  base 256x256 img1	canvas2: base 256x256 dst				react [					lower: box1/color					upper: box2/color 					rcvInrange src dst lower upper				]	do [urSl/data: ugSl/data: ubSl/data: 1.0]]